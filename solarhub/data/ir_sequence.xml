<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="seq_solarpanel_id" model="ir.sequence">
        <field name="name">Solar</field>
        <field name="code">solar.panel</field>
        <field name="prefix">#SOL-</field>
        <field name="padding">4</field>
    </record>

    <record id="seq_bat_id" model="ir.sequence">
        <field name="name">Battery</field>
        <field name="code">solar.battery</field>
        <field name="prefix">#BAT-</field>
        <field name="padding">4</field>
    </record>

    <record id="seq_inverter" model="ir.sequence">
        <field name="name">Inverter</field>
        <field name="code">inverter</field>
        <field name="prefix">#INV-</field>
        <field name="padding">4</field>
    </record>

    <record id="seq_solarorder" model="ir.sequence">
        <field name="name">Solar Order</field>
        <field name="code">solarhub.order</field>
        <field name="prefix">#ORD-</field>
        <field name="padding">4</field>
    </record>
</odoo>